#ifndef HAND_H_
#define HAND_H_

#include <vector>
#include <string>
#include "Card.h"

class Hand {
 public:
 Hand(const std::Vector<string> hand): m_cards{hand} {}

  std::Vector get_hand() const {return m_cards;}
  void set_hand(const Vector<string>& hand) {m_cards = hand;} 


 private:
  std::Vector<Card> m_cards;


}


#endif
